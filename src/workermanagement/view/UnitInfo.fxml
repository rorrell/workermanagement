<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="700.0" styleClass="background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="workermanagement.controller.UnitInfoController">
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
   <children>
      <HBox alignment="CENTER" layoutX="323.0" layoutY="20.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="20.0">
         <children>
            <Label fx:id="titleLbl" layoutX="323.0" layoutY="30.0" styleClass="title" text="Label" AnchorPane.topAnchor="20.0" />
         </children>
      </HBox>
      <HBox layoutX="25.0" layoutY="43.0" spacing="10.0">
         <children>
            <Button fx:id="cancelBtn" layoutX="29.0" layoutY="43.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Cancel" />
            <Button fx:id="saveBtn" layoutX="93.0" layoutY="43.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Save" />
         </children>
      </HBox>
      <GridPane layoutX="253.0" layoutY="160.0" styleClass="grid" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <HBox spacing="10.0" styleClass="box" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER">
               <children>
                  <Label fx:id="nameLbl" styleClass="required" text="Name:" />
                  <TextField fx:id="nameTxt" />
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
</AnchorPane>
